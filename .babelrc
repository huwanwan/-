{
  "presets": [
   	["es2015", { "modules": false }],
    "react",
    "latest",
    "stage-2",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ]
  ],
  "plugins": [
    ["transform-decorators-legacy"],
    [
    "import",
    {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    },
    "transform-runtime",
   [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ]]// `style: true` 会加载 less 文件
}